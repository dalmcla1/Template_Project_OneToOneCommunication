<?xml version="1.0" encoding="utf-8"?>
<QuestionSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="education_questions">
  <Questions>
    <InfoScreen Name="education_info" NColumns="1" NRows="1">
      <Text><![CDATA[<b><size=48>Education questionnaire</size></b>

Questions about your educational background]]></Text>
    </InfoScreen>
    <ChoiceQuestion Name="socDem11" NColumns="1" NRows="2" Choice="Single">
      <Jumps>
        <Jump Destination="socDem12" Activator="FT" />
      </Jumps>
      <Text><![CDATA[Are you currently a student or postdoctoral fellow?]]></Text>
      <RowLabels>
        <Label Output="1">
          <Text><![CDATA[Yes]]></Text>
        </Label>
        <Label Output="0">
          <Text><![CDATA[No]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem11_1" NColumns="1" NRows="6" Choice="Single">
      <Text><![CDATA[Are you a full time student or postdoctoral fellow?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[Technical training program]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Full time undergraduate student]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Part time undergraduate student]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Full time graduate student]]></Text>
        </Label>
        <Label Output="4">
          <Text><![CDATA[Part time graduate student]]></Text>
        </Label>
        <Label Output="5">
          <Text><![CDATA[Postdoctoral fellow]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem12" NColumns="1" NRows="8" Choice="Single">
      <Jumps>
        <Jump Destination="*" Activator="TFFFFFFF" />
        <Jump Destination="socDem12_2" Activator="FFTFFFFF" />
        <Jump Destination="*" Activator="FFFTFFFF" />
        <Jump Destination="*" Activator="FFFFTFFF" />
        <Jump Destination="socDem12_3" Activator="FFFFFTFF" />
        <Jump Destination="socDem12_4" Activator="FFFFFFTF" />
        <Jump Destination="*" Activator="FFFFFFFT" />
      </Jumps>
      <Text><![CDATA[What is the highest level of education you have completed?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[No training completed]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Compulsory education (Obligatorische Schule )]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Professional education]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Commercial middle school (Handelsmittelschule)]]></Text>
        </Label>
        <Label Output="4">
          <Text><![CDATA[High school / Gymnasium / Kantonsschule (Matur)]]></Text>
        </Label>
        <Label Output="5">
          <Text><![CDATA[University of applied sciences (Fachhochschule)]]></Text>
        </Label>
        <Label Output="6">
          <Text><![CDATA[University degree]]></Text>
        </Label>
        <Label Output="7" Answerable="true">
          <Text><![CDATA[Other:]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem12_1" NColumns="1" NRows="6" Choice="Single">
      <Jumps>
        <Jump Destination="*" Activator="*" />
      </Jumps>
      <Text><![CDATA[What level of compulsory education?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[Primary (Primarschule)]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Elementary (Realschule)]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Upper elementary school (Sekundarschule)]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Orientation year (10. Schuljahr)]]></Text>
        </Label>
        <Label Output="4" Answerable="true">
          <Text><![CDATA[Other:]]></Text>
        </Label>
        <Label Output="99">
          <Text><![CDATA[Don’t know]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem12_2" NColumns="1" NRows="10" Choice="Single">
      <Jumps>
        <Jump Destination="*" Activator="*" />
      </Jumps>
      <Text><![CDATA[What level of professional education?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[Au-Pair year (Au-Pair Jahr)]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Pre-apprenticeship (Anlehre)]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Apprenticeship diploma (Lehrabschluss)]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Commercial school (Handelsschule)]]></Text>
        </Label>
        <Label Output="4">
          <Text><![CDATA[Technical School (TS Technikerschule) or other technical school (e.g., HKG)]]></Text>
        </Label>
        <Label Output="5">
          <Text><![CDATA[Federal vocational baccalaureate (Lehrabschluss mit Berufsmaturität)]]></Text>
        </Label>
        <Label Output="6">
          <Text><![CDATA[Teacher Seminar (Lehrkräfteseminar)]]></Text>
        </Label>
        <Label Output="7">
          <Text><![CDATA[Nursing degree (Krankenschwesterausbildung)]]></Text>
        </Label>
        <Label Output="8" Answerable="true">
          <Text><![CDATA[Other:]]></Text>
        </Label>
        <Label Output="99">
          <Text><![CDATA[Don’t know]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem12_3" NColumns="1" NRows="9" Choice="Single">
      <Jumps>
        <Jump Destination="*" Activator="*" />
      </Jumps>
      <Text><![CDATA[What degree did you get at the University of applied sciences?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[Bachelor of Science FH]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Master of Science FH]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Certificate of Advanced Studies (CAS)]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Diploma of Advanced Studies (DAS)]]></Text>
        </Label>
        <Label Output="4">
          <Text><![CDATA[Executive MBA]]></Text>
        </Label>
        <Label Output="5">
          <Text><![CDATA[Master of Advanced Studies (MAS)]]></Text>
        </Label>
        <Label Output="6">
          <Text><![CDATA[Seminar]]></Text>
        </Label>
        <Label Output="7" Answerable="true">
          <Text><![CDATA[Other:]]></Text>
        </Label>
        <Label Output="99">
          <Text><![CDATA[Don’t know]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem12_4" NColumns="1" NRows="6" Choice="Single">
      <Jumps>
        <Jump Destination="*" Activator="FFFFFT" />
      </Jumps>
      <Text><![CDATA[What level of University?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[Diploma (e.g. Lic.phil., Lic.nat., Med.)]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Bachelor]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Masters (M c., MA)]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Ph.D.]]></Text>
        </Label>
        <Label Output="4">
          <Text><![CDATA[Dr. (e.g. Dr. med., Dr. med. dent., Dr. nat.)]]></Text>
        </Label>
        <Label Output="5" Answerable="true">
          <Text><![CDATA[Other:]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="socDem12_4_1" NColumns="1" NRows="7" Choice="Single">
      <Text><![CDATA[Did you pursue any Advanced Studies degree (Nachdiplomstudium)? If yes, what level?]]></Text>
      <RowLabels>
        <Label Output="0">
          <Text><![CDATA[No]]></Text>
        </Label>
        <Label Output="1">
          <Text><![CDATA[Certificate of Advanced Studies (CAS)]]></Text>
        </Label>
        <Label Output="2">
          <Text><![CDATA[Diploma of Advanced Studies (DAS)]]></Text>
        </Label>
        <Label Output="3">
          <Text><![CDATA[Executive MBA]]></Text>
        </Label>
        <Label Output="4">
          <Text><![CDATA[Master of Advanced Studies (MAS)]]></Text>
        </Label>
        <Label Output="5">
          <Text><![CDATA[Seminars]]></Text>
        </Label>
        <Label Output="6" Answerable="true">
          <Text><![CDATA[Other:]]></Text>
        </Label>
      </RowLabels>
    </ChoiceQuestion>
  </Questions>
</QuestionSet>